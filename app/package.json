{
  "name": "iam",
  "version": "1.0.5",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
    "typeorm": "node ./node_modules/typeorm/cli.js -d ./dist/config/db.config.js",
    "typeorm:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/config/db.config.ts"
  },
  "dependencies": {
    "app-life-cycle-pkg": "git+git@github.com:spalx/app-life-cycle-pkg.git#1.0.6",
    "common-loggers-pkg": "git+git@github.com:spalx/common-loggers-pkg.git#1.0.3",
    "http-transport-adapter": "git+git@github.com:spalx/http-transport-adapter.git#1.0.6",
    "iam-pkg": "git+git@github.com:spalx/iam-pkg.git#1.2.7",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rest-pkg": "git+git@github.com:spalx/rest-pkg.git#1.0.3",
    "service-discovery-pkg": "git+git@github.com:spalx/service-discovery-pkg.git#1.0.2",
    "transport-pkg": "git+git@github.com:spalx/transport-pkg.git#1.1.7",
    "typeorm": "^0.3.21",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
