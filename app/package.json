{
  "name": "iam",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'"
  },
  "dependencies": {
    "app-life-cycle-pkg": "git+git@github.com:spalx/app-life-cycle-pkg.git#1.0.5",
    "common-loggers-pkg": "git+git@github.com:spalx/common-loggers-pkg.git#1.0.3",
    "http-transport-adapter": "git+git@github.com:spalx/http-transport-adapter.git#1.0.1",
    "iam-pkg": "git+git@github.com:spalx/iam-pkg.git#1.2.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "rest-pkg": "git+git@github.com:spalx/rest-pkg.git#1.0.3",
    "transport-pkg": "git+git@github.com:spalx/transport-pkg.git#1.0.9",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.13.10",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  }
}
